{
  "expo": {
    "name": "time-calculator",
    "slug": "time-calculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./time-calculator-app/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./time-calculator-app/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./time-calculator-app/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "userInterfaceStyle": "automatic"
    },
    "web": {
      "favicon": "./time-calculator-app/assets/images/favicon.png"
    }
  }
}
